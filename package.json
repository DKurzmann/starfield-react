  "name": "starfield-react",
  "version": "2.0.0",
  "description": "A react component that draws a starfield on a canvas element.",
  "author": "Christopher Decoster",
  "license": "MIT",
  "repository": "https://github.com/impaler/starfield-react",
  "main": "dist/starfield.cjs.js",
